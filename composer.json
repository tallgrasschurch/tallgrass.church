{
  "name": "tallgrasschurch/tallgrass.church",
  "type": "project",
  "description": "Tallgrass Community Church",
  "homepage": "http://tallgrass.church",
  "authors": [
    {
      "name": "D. Nathan Dillon",
      "role": "Developer",
      "homepage": "https://natedillon.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "drupal-composer/drupal-scaffold": "^2.2",
    "cweagans/composer-patches": "^1.6",
    "drupal/console": "~1.0",
    "drupal/core": "~8.7.0",
    "drush/drush": "~9.0",
    "drupal/devel": "^1.2",
    "drupal/pathauto": "^1.0",
    "drupal/google_analytics": "^2.4",
    "drupal/insert": "^1.0",
    "drupal/field_group": "^3.0",
    "drupal/address": "^1.3",
    "drupal/twig_tweak": "^2.0",
    "drupal/captcha": "^1.0-beta1",
    "drupal/recaptcha": "^2.3",
    "drupal/convert_bundles": "^1.0",
    "drupal/podcast": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "patches": {
      "drupal/core": {
        "Add option to show only start or end date in the DateTime Range custom formatter": "https://www.drupal.org/files/issues/2018-05-23/drupal-display-one-date-formatter-2827055-25.patch",
        "RSS view with fields give wrong URL from path field": "https://www.drupal.org/files/issues/2019-01-18/2673980-87.views-rss-fields-link.functional-test-only.patch"
      },
      "drupal/podcast": {
        "itunes:image should be output as an href attribute": "https://www.drupal.org/files/issues/2019-05-06/podcast-setup_image-3034933-3.patch",
        "iTunes Category with ampersand outputs extra 'amp;' text": "https://www.drupal.org/files/issues/2019-03-27/3029877-2.patch"
      }
    }
  }
}
